"type": "object",
"properties": {
{{#each properties}}
  "{{name}}": {
  {{#if comment}}
    "description": "{{{escape comment}}}",
  {{/if}}
  {{#if tags}}
    {{#each tags}}
      {{{keyword ../type this}}}
    {{/each}}
  {{/if}}
  {{> type this }}
  }{{#unless @last}},{{/unless}}
{{/each}}
},
"additionalProperties": false,
"required": [{{{required properties}}}]
